{
    "name": "grapesjs-tailwind",
    "version": "1.0.15",
    "description": "Grapesjs Tailwind",
    "main": "dist/grapesjs-tailwind.min.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/Ju99ernaut/grapesjs-tailwind.git"
    },
    "scripts": {
        "start": "set NODE_OPTIONS=--openssl-legacy-provider && grapesjs-cli serve",
        "build": "set NODE_OPTIONS=--openssl-legacy-provider && grapesjs-cli build",
        "bump": "set NODE_OPTIONS=--openssl-legacy-provider && npm version patch -m 'Bump v%s'"
    },
    "keywords": [
        "grapesjs",
        "plugin"
    ],
    "devDependencies": {
        "grapesjs-cli": "^2.0.0"
    },
    "license": "MIT",
    "dependencies": {
        "css": "^3.0.0"
    }
}
